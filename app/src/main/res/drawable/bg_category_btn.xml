<!-- res/drawable/bg_search.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- Màu nền trong suốt hoặc màu khác nếu muốn -->
    <solid android:color="@android:color/transparent"/>

    <!-- Viền màu #627281, độ dày 2dp (có thể thay đổi) -->
    <stroke
        android:width="1dp"
        android:color="#846046" />

    <!-- Bo góc 100dp -->
    <corners android:radius="33dp"/>
</shape>